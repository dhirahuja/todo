    <%= form_for([@plum, @plum.projects.build]) do |f| %>
        <p>
          <%= f.label :name %><br>
          <%= f.text_field :name %>
        </p>
        <p>
          <%= f.label :number %><br>
          <%= f.text_field :number %>
        </p>
        <p>
          <%= f.label :department %><br>
          <%= f.text_field :department %>
        </p>
        <p>
        <%= f.label :state %><br>
        <%= f.select :state, [['active'], ['inactive'], ['disable']] %>
        </p>
        <p>
          <%= f.label :address %><br>
          <%= f.text_area :address %>
        </p>
        <p>
          <%= f.submit %>
        </p>
      <% end %>